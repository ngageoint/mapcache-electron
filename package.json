    "@terraformer/arcgis": "^2.1.2",
    "@turf/area": "^6.5.0",
    "@turf/bbox": "^6.5.0",
    "@turf/bbox-clip": "^6.5.0",
    "@turf/bbox-polygon": "^6.5.0",
